 function filterProducts() {
            
  var city = <?php echo $new;?>;
   var type = '<?php echo $type;?>';
   
 
    var collegename,amenities_tv,amenities_wifi,amenities_gym,amenities_electricity,roomtype,price_range,postroom_boy,postroom_girl,postroom_family,postroom_anyone,data,roommate_student,roommate_jobperson,roommate_nomatter,roommate_male,roommate_female,roommate_either;
    
    

          price_range = $('.price_range').val();

          collegename = $('#getcollegeEnter').val();
          roomtype = multiple_values('roomtype');
             

            
             amenities_tv = multiple_values('common_amenities');
             amenities_wifi = multiple_values('common_amenities');
             amenities_gym = multiple_values('common_amenities');
             amenities_electricity = multiple_values('common_amenities');



             postroom_boy = multiple_values('postroom_roomfor');
             postroom_girl = multiple_values('postroom_roomfor');
             postroom_family = multiple_values('postroom_roomfor');
             postroom_anyone = multiple_values('postroom_roomfor');

            

             roommate_student = multiple_values('roommate_roomfor');
             roommate_jobperson = multiple_values('roommate_roomfor');
             roommate_nomatter = multiple_values('roommate_roomfor');

             roommate_male = multiple_values('roommate_ideal');
             roommate_female = multiple_values('roommate_ideal');
             roommate_either = multiple_values('roommate_ideal');

             var avltype = document.getElementById('avltype').value;
             var avldate = document.getElementById('avldateVal').value;
              
        if (type == 'postroom') {
    
    data = {city:city,type:type,roomtype:roomtype,price_range:price_range,postroom_boy:postroom_boy,postroom_girl:postroom_girl,postroom_family:postroom_family,postroom_anyone:postroom_anyone,avltype:avltype,avldate:avldate,amenities_tv:amenities_tv,amenities_wifi:amenities_wifi,amenities_gym:amenities_gym,amenities_electricity:amenities_electricity};
} else {
    
    data =  {city:city,type:type,roomtype:roomtype,price_range:price_range,roommate_student:roommate_student,roommate_jobperson:roommate_jobperson,roommate_nomatter:roommate_nomatter,roommate_male:roommate_male,roommate_female:roommate_female,roommate_either:roommate_either,avltype:avltype,avldate:avldate,amenities_tv:amenities_tv,amenities_wifi:amenities_wifi,amenities_gym:amenities_gym,amenities_electricity:amenities_electricity,collegename:collegename};
}
            
            $.ajax({
                url:"ajax.php",
                type:'post',
                data:data,

            beforeSend: function () {
            
        },
                success:function(result){
                    
                      $('.product-data').html(result);
                    var result = document.getElementById("map").value;
                   

                    
                }
            });

    }
  
    function multiple_values(inputclass){
        var val = new Array();
        $("."+inputclass+":checked").each(function() {
            val.push($(this).val());

        });
        return val;
    }
    
$(function() {
$('.price_range').jRange({
    from: 0,
    to: 10000,
    step: 50,
    format: '$%s USD',
    width: 250,
    showLabels: true,
    isRange : true
});
});
  
</script>

<script type="text/javascript">

function bookmark(id,type) { 
   

var xhttp = new XMLHttpRequest();

var param = "postroom_id="+id+"&type="+type;


console.log(param);



xhttp.onreadystatechange = function() {//Call a function when the state changes.

   if (this.readyState == 4 && this.status == 200) {
      document.getElementById("wishbtn"+id).innerHTML = this.responseText;
      
    }
};

xhttp.open("POST", "http://103.10.234.192/~tirbhinnat/roomies/postroom/addtowishlist.php", true);
xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
xhttp.send(param);
}

console.error();
</script>
<script type="text/javascript">
$(document).ready(function(){
    $("#getcollegeEnter").keyup(function(){
        var getcollegeEnter = $(this).val();
        if(getcollegeEnter != "" && getcollegeEnter.length > 2) {
        $.ajax({
            type: "POST",
            url: "gecollegename.php",
            data: 'getcollegeEnter='+getcollegeEnter,
            dataType: "html",
            async: false,
            // success: function(text){
            //     $('#search_box').html(text);
            // }
            
            success: function(resp) {
          if(resp != "") {
            $("#search_box").html(resp).show();  
          } else {
            $("#search_box").html(resp).hide();
          }
        }
        });
    }
        else {
      $("#search_box").html("").hide();
    }

  });

 
    
});
function selectCollege(val) {
$("#getcollegeEnter").val(val);
$("#search_box").hide();
}
</script>
 
<script type="text/javascript">
var getimage = [];
var html = [];
var getprice = [];

<?php
 

foreach($rent as $key => $val)
{
 
?>
 

   
  getprice[<?php echo $rent[$key];?>] = '<?php echo $rent[$key];?>';
  
  
  html[<?php echo $key;?>] = '<div class="col-sm-12"><div style="float:left">'+getprice[<?php echo $rent[$key];?>]+'</div>';
<?php
