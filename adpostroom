$postroom_girl = isset($_POST['postroom_girl'])?$_POST['postroom_girl']:"";
$postroom_family = isset($_POST['postroom_family'])?$_POST['postroom_family']:"";
$postroom_anyone = isset($_POST['postroom_anyone'])?$_POST['postroom_anyone']:"";


$roommate_student="";
$roommate_jobperson="";
$roommate_nomatter="";

$roommate_student = isset($_POST['roommate_student'])?$_POST['roommate_student']:"";
$roommate_jobperson = isset($_POST['roommate_jobperson'])?$_POST['roommate_jobperson']:"";
$roommate_nomatter = isset($_POST['roommate_nomatter'])?$_POST['roommate_nomatter']:"";


$roommate_male="";
$roommate_female="";
$roommate_either="";

$roommate_male = isset($_POST['roommate_male'])?$_POST['roommate_male']:"";
$roommate_female = isset($_POST['roommate_female'])?$_POST['roommate_female']:"";
$roommate_either = isset($_POST['roommate_either'])?$_POST['roommate_either']:"";

$avldate = isset($_POST['avldate'])?$_POST['avldate']:"";



if($_POST['avltype']!= '')
{
  $avldate = date("Y-m-d");
}

if($type=='postroom'){
$query = "SELECT amenities.amenities AS Amenities, cities.name AS City,postroom.postroom_id AS Postroom_id,roomtype.roomtype AS Roomtype,bathroom.bathroom AS Bathroom,addcustomer.age AS AGE,addcustomer.image AS Image, addcustomer.username AS Username,postroom.monthlyrent AS Rent,postroom.user_id AS User_id,postroom.address AS Address,postroom_roomfor.postroom_roomfor AS Interest_gender,postroom.type AS Type,postroom.movedate AS Movedate,postroom.latitude AS latitude,postroom.longitude AS longitude,postroom.currrentdate AS Postdate FROM `cities`,`roomtype`,`postroom`,`bathroom`,`amenities`,`postroom_roomfor`,`addcustomer` WHERE postroom_roomfor.id=postroom.interest_gender  AND amenities.id=postroom.amenities AND bathroom.id=postroom.bathroom AND roomtype.id=postroom.roomtype  AND cities.id=postroom.city  AND addcustomer.id=postroom.user_id AND type='$type' AND city='$city'";

}
else{
 $query = "SELECT amenities.amenities AS Amenities,cities.name AS City,postroom.postroom_id AS Postroom_id,roomtype.roomtype AS Roomtype,bathroom.bathroom AS Bathroom,addcustomer.age AS AGE,addcustomer.image AS Image, addcustomer.username AS Username,postroom.collegename AS Collegename,postroom.monthlyrent AS Rent,postroom.user_id AS User_id,roomfor.roomfor AS Roommate_roomfor,roommate_idealperson.roommate_idealperson AS Idealroommate,postroom.latitude AS latitude,postroom.longitude AS longitude,postroom.address AS Address,postroom.movedate AS Movedate,postroom.type AS Type,postroom.currrentdate AS Postdate FROM `cities`,`roomtype`,`postroom`,`bathroom`,`roomfor`,`amenities`,`roommate_idealperson`,`addcustomer` WHERE roommate_idealperson.id=postroom.idealroommate AND amenities.id=postroom.amenities AND bathroom.id=postroom.bathroom AND roomfor.id=postroom.roomfor AND roomtype.id=postroom.roomtype  AND addcustomer.id=postroom.user_id AND cities.id=postroom.city AND type='$type' AND city='$city'";
 }   
            
           

					  if(!empty($roomtype)){
						  $colordata =implode("','",$roomtype);
						  $query  .= " and postroom.roomtype in('$colordata')"; 
					  }
            
             if(!empty($amenities_tv)){
              $amenities_tvdata =implode("','",$amenities_tv);
              $query  .= " and postroom.amenities in('$amenities_tvdata')"; 
            }

             if(!empty($amenities_wifi)){
              $amenities_wifidata =implode("','",$amenities_wifi);
              $query  .= " and postroom.amenities in('$amenities_wifidata')"; 
            }

             if(!empty($amenities_gym)){
              $amenities_gymdata =implode("','",$amenities_gym);
              $query  .= " and postroom.amenities in('$amenities_gymdata')"; 
            }

             if(!empty($amenities_electricity)){
              $amenities_electricitydata =implode("','",$amenities_electricity);
              $query  .= " and postroom.amenities in('$amenities_electricitydata')"; 
            }


            
					  
            if(!empty($price_range)){
        $priceRangeArr = explode(',', $price_range);
        $query  .= "and postroom.monthlyrent BETWEEN '".$priceRangeArr[0]."' AND '".$priceRangeArr[1]."'";
        
        }
           $query  .= "and postroom.collegename LIKE '%".$collegename."%' ";
           $query .="and postroom.movedate <='$avldate'";
            
            if($type == 'postroom'){

             if(!empty($postroom_boy)){
                          $postroom_boy_data =implode("','",$postroom_boy);
                           $query  .= " and postroom.interest_gender in('$postroom_boy_data')"; 
              }



        if(!empty($postroom_girl)){
                          $postroom_girl_data =implode("','",$postroom_girl);
                           $query  .= " and postroom.interest_gender in('$postroom_girl_data')"; 
              }


                 if(!empty($postroom_family)){
                          $postroom_family_data =implode("','",$postroom_family);
                           $query  .= " and postroom.interest_gender in('$postroom_family_data')"; 
              }


               if(!empty($postroom_anyone)){
                          $postroom_anyone_data =implode("','",$postroom_anyone);
                           $query  .= " and postroom.interest_gender in('$postroom_anyone_data')"; 
              }
             }

             else if($type =='roommate'){ 
               
               if(!empty($roommate_student)){
                          $roommate_student_data =implode("','",$roommate_student);
                           $query  .= " and postroom.roomfor in('$roommate_student_data')"; 
              }



        if(!empty($roommate_jobperson)){
                          $roommate_jobperson_data =implode("','",$roommate_jobperson);
                           $query  .= " and postroom.roomfor in('$roommate_jobperson_data')"; 
              }


                 if(!empty($roommate_nomatter)){
                          $roommate_nomatter_data =implode("','",$roommate_nomatter);
                           $query  .= " and postroom.roomfor in('$roommate_nomatter_data')"; 
              }

              if(!empty($roommate_male)){
                          $roommate_male_data =implode("','",$roommate_male);
                           $query  .= " and postroom.idealroommate in('$roommate_male_data')"; 
              }

if(!empty($roommate_female)){
                          $roommate_female_data =implode("','",$roommate_female);
                           $query  .= " and postroom.idealroommate in('$roommate_female_data')"; 
