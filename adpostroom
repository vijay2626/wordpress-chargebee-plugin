<script type="text/javascript">
	
	function checkIcon(elm)
	{      
		var x = elm.innerHTML;

		if(x.indexOf('color:#999') != -1)
		{
			
			x = x.replace('color:#999','color:#FDC600');
		
			x = x.replace('color:#999','color:#FDC600');
		   
			var t = document.getElementById('selectedAmenities').value;
			t = t+"-"+elm.lang+"-,";
		}
		else
		{
	
			x = x.replace('color:#FDC600','color:#999');
		
			x = x.replace('color:#FDC600','color:#999');
		

			var t = document.getElementById('selectedAmenities').value;
			var toRemove = '-'+elm.lang+'-,';
			t = t.replace(toRemove,'');

		}
		elm.innerHTML = x;
		document.getElementById('selectedAmenities').value = t;

	}

