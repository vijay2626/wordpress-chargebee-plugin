$type = isset($_POST['type'])?$_POST['type']:"";
$roomtype = isset($_POST['roomtype'])?$_POST['roomtype']:"";
$price_range = isset($_POST['price_range'])?$_POST['price_range']:"";


$amenities_tv = isset($_POST['amenities_tv'])?$_POST['amenities_tv']:"";
$amenities_wifi = isset($_POST['amenities_wifi'])?$_POST['amenities_wifi']:"";
$amenities_gym = isset($_POST['amenities_gym'])?$_POST['amenities_gym']:"";
$amenities_electricity = isset($_POST['amenities_electricity'])?$_POST['amenities_electricity']:"";



$postroom_boy="";
$postroom_girl="";
$postroom_family="";
$postroom_anyone="";

